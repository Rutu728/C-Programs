#include<stdio.h>
#include<conio.h>

int main()

{
 int Sp=0,Ep=0,i=0,j=0;
    printf("\n Enter Start Number:");
    scanf("%d",&Sp);
    printf("\n Enter End Number:");
    scanf("%d",&Ep);
    printf("\n==============\n");
    if(Sp<=Ep)
    {
     for(i=1;i<=10;i++)
      {
        for(j=Sp;j<=Ep;j++)
      {
       printf("%3d",i*j);
      }
      printf("\n");
    }
  }
  else
   {
    for(i=1;i<=10;i++)
    {
      for(j=Sp;j>Ep;j++)
    {
      printf("%3d",i*j);
    }
    printf("\n");
    }
  }
   printf("\n=========\n");
    getch();
    return 0;
}