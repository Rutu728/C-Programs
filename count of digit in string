#include<stdio.h>
#include<conio.h>

int main()

{
  char cSrc[20]="";
  int i=0,Dig_cnt=0;
    printf("\n Enter a string=");
    gets(cSrc);
    
    while(cSrc[i]!='\0')
    {
      if(cSrc[i]>='0'&& cSrc[i]<='9')
      {
        Dig_cnt++;
      }
        i++;
    }
    printf("\n Digits count in Given String is =%d",Dig_cnt);
    getch();
    return 0;
    
}