#include<stdio.h>
#include<conio.h>

int main()

{
 int No=0,max=0,i=0;
    printf("\n Enter 5 Integer Number to find maximum from it=>\n");
    for(i=1;  i<=5   ;i++)
    {
     printf("\n Enter Number %d",i);
     scanf("%d",&No);
     
    if((No>max)||(i==1))
    {
      max=No;
    }
     
  }
   printf("\n maximum from given 5 Numbers in %d is=%d",max);
    getch();
    return 0;
}  
 
 
    