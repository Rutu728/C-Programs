#include<stdio.h>
#include<conio.h>

int main()

{
   int No1=0,No2=0,Pow=0,i=0;

   printf("\n Enter to Number to find 1st Raise to second=");
   scanf("%d%d",&No1,&No2);

    for(i=0,Pow=1; i<No2; i++)
    {
      Pow = Pow*No1;
    }
    printf("\n (%d)^%d=%d",No1,No2,Pow);
    getch();
    return 0;
}
   
