#include<stdio.h>
#include<conio.h>
int main ()
{
    
   int No = 0;
   printf("\n Enter a Number to check it is Positive or Negative:");

   scanf("%d",&No);

   if(No==0)
    {
      printf("\n Number is Neutral...");
      goto DWN;
    }
    
    if(No > 0)
    
    {
      printf("\n %d is Positive",No);
    }
    
    else 
    {
      printf("\n %d is Negative",No);
    }
    
    DWN:
    
    printf("\n Thanks for working with us!!!");
    
    getch();
    return 0;
}