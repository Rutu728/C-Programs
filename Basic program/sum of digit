#include<stdio.h>
#include<conio.h>

int main()

{
  int No = 0,Temp = 0,Dig = 0,Sum =0;

  printf("\n Enter The Number");
  scanf("%d",&No);
 
  if(No < 0)
  {
    printf("\n Invalide Number");
    return -1;
  }
 
   Temp = No;
   while(Temp > 0)
  
  {
    Dig = Temp%10;
    Sum = Sum+Dig;
    Temp = Temp/10;
  }

   printf("\n The Sum of Digit in %d is %d",No,Sum);
   getch();
   return 0;
}

   
  
