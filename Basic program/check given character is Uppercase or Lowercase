#include<stdio.h>
#include<conio.h>

 int main()
{
  
  char ch='\0';
  printf("\n\n Enter a character to check wether it is Upper or Lower case:");
  
  ch=getch();
  
  if(ch>='A'&& ch<='Z')
  
  {
    printf("\n Given character is Uppercase.");
  }

   else if(ch>='a'&& ch<='z')

  {
    printf("\n Given character is Lowercase");
  }

    else 
  {
    printf("\n Given character is not Alphabet");
  }
 
   getch();
   return 0;
}