#include<stdio.h>
#include<conio.h>

int main()

{
    char cSrc[20]="";
    int i=0,j=0;
    
    printf("\n Enter your city name=");
    gets(cSrc);
    while(cSrc[i]!='\0')
    {
      i++;
    }
      j++;
    while(j>i)
    {
      if(cSrc[i]!=cSrc[i])
      {
        break;
      }
      j--;
      i++;
    }
     if(i>j)
    {
      printf("\n Given sting is not palindrom");
    }
      else
    {
      printf("\n Given string is palindrom");
    }
      getch();
     return 0;
}