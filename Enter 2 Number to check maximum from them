#include<stdio.h>
#include<conio.h>

int main()

{
  int No1=0,No2=0;
  
  printf("\n Enter 2 Numbers to check Maximum from them:");
  scanf("%d%d",&No1,&No2);

  if((No1==No2))
   {
     printf("\n 2 Numbers are Equal..");
   }

   else if((No1>=No2))
   {
     printf("\n %d is Maximum",No1);
   }

   else if((No2>=No1))
   {
     printf("\n %d is Maximum",No2);
   }

   else
   {
      printf("\n Invalide");
   }

  printf("\n=====================\n");
    getch();
    return 0;
}