#include<stdio.h>
#include<conio.h>

int main()

{
   char cSrc[20]= "";
   char cDest[20]="";

   int i=0,j=0;
 
   printf("\n Enter your city name=");
   gets(cSrc);

   while(cSrc[i]!='\0')
   { 
     i++;
   }
 
   i=i-1;
   while(i>=0)

  {
    cDest[j]=cSrc[i];
    i--;
    j++;
  }

    cDest[j]='\0';
    
    printf("\n New string After Reversal=%s",cDest);
    getch();
    return 0;
}