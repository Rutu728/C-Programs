#include<stdio.h>
#include<conio.h>

int main()

{
  char ch = '\0';
  printf("\n Enter a character:");
  ch = getche();

  if(ch>='A'&& ch<='Z')
   {
     printf("\n Given character is Uppercase.");
   }

  else if(ch>='a'&& ch<='z')
   {
     printf("\n Given character is Lowercase.");
   }

  else if(ch>='0'&& ch<=
'9')
   {
     printf("\n Given character is Digit.");
   }

  else
    {
      printf("\n Given character is special symbol.");
    }
    
    getch();
    return 0;
}