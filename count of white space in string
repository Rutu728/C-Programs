#include<stdio.h>
#include<conio.h>

int main()

{
  char cSrc[20]="";
  int i=0,W_cnt=0;
    printf("\n Enter a string=");
    gets(cSrc);
    
    while(cSrc[i]!='\0')
    {
      if(cSrc[i]==' ')
      {
       W_cnt++;
      }
        i++;
    }
    printf("\n White Spece count in Given String is =%d",W_cnt);
    getch();
    return 0;
    
}