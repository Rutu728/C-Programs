#include<stdio.h>
#include<conio.h>

int main()

{
  int No=0,Temp=0,Dig=0,O_Cnt=0;
  printf("\n Enter the Number=");
  scanf("%d",&No);

  Temp=No;
  if(Temp<0)
  {
    Temp= -1*Temp;
  }
   while (Temp>0)
   {
   Dig=Temp%10;
   if(Dig %2==1)
    {
      O_Cnt++;
    }
    
    printf("\n Digit=%d  Odd Count =%d",Dig,O_Cnt);
    Temp=Temp/10;
  }
   printf("\n\n Count of Odd Digit in %d is =%d",No,O_Cnt);
   getch();
   return 0;

}
    
    
   
