#include<stdio.h>
#include<conio.h>

int main()

{
  int No = 0;

    printf("\n Enter a Number to check whether it is divisible by 5&7:");
    scanf("%d",&No);
    if(No % 5 == 0 && No %7 == 0)
    
    {
      printf("\n Given Number %d is Divisible by Both 5&7",No);
    }
    
    else if(No %5 == 0)
    {
      printf("\n Given Number %d is only Divisible by 5",No);
    }
    
    else if(No %7 == 0)
    {
      printf("\n Given Number %d is only Divisible by 7",No);
    }
     else
    {
      printf("\n Given Number %d is Neither Divisible By 5 Nor By 7",No);
    }
     
     printf("\n Thanks!!!");
     getch();
     return 0;
}