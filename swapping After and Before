#include<stdio.h>
#include<conio.h>

 int main()
{
  
  int No1=0,No2=0,Temp=0;
  printf("\n Enter 2 Numbers for swapping:");
  scanf("%d%d",&No1,&No2);

  printf("\n Given Numbers Before swap=>\n\t Number1=%d \t Number2=%d",No1,No2);

  getch();
  Temp=No1;
  No1=No2;
  No2=Temp;

  printf("\n\n Given Numbers After swap=>\n\t Number1=%d\t Number2=%d",No1,No2);

  getch();
  return 0;
}