#include<stdio.h>
#include<conio.h>

int main()

{
    char cSrc[20]="";
    int i=0,Wd_cnt=0;
    
    printf("\n Enter a sting=");
    gets(cSrc);
    
    while(cSrc[i]!='\0')
    {
      if(cSrc[i]==' ')
     {
      i++;
      continue;
     }
     else
    {
      while(cSrc[i]!=' '&& cSrc[i]!='\0')
      {
        i++;
      }
       Wd_cnt++;
    }
  }
  printf("\n word count in Given string is =%d",Wd_cnt);
     getch();
     return 0;
}