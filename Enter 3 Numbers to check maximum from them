#include<stdio.h>
#include<conio.h>

int main()

{
  int No1=0,No2=0,No3=0;
  
  printf("\n Enter 3 Numbers to check Maximum from them:");
  scanf("%d%d%d",&No1,&No2,&No3);

  if((No1==No2)&&(No2==No3))
   {
     printf("\n All 3 Numbers are Equal..");
   }

   else if((No1>=No2)&&(No1>=No3))
   {
     printf("\n %d is Maximum",No1);
   }

   else if((No2>=No1)&&(No2>=No3))
   {
     printf("\n %d is Maximum",No2);
   }

   else if((No3>=No1)&&(No3>=No2))
   {
     printf("\n %d is Maximum",No2);
   }

  else
   {
      printf("\n Invalide");
   }

  printf("\n=====================\n");
    getch();
    return 0;
}